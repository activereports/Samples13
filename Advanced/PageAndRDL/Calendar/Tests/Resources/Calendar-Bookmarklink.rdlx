<Report xmlns="http://schemas.microsoft.com/sqlserver/reporting/2005/01/reportdefinition">
	<Body>
		<Height>9.525cm</Height>
		<ReportItems>
			<CustomReportItem Name="Calendar1">
				<Type>Calendar</Type>
				<Top>1.905cm</Top>
				<Left>3.175cm</Left>
				<Width>3.5in</Width>
				<Height>3in</Height>
				<CustomData>
					<DataSetName>DataSet1</DataSetName>
					<DataRowGroupings>
						<DataGroupings>
							<DataGrouping>
								<CustomProperties>
									<CustomProperty>
										<Name>calendar:Event.Value</Name>
										<Value>=Fields!Value.Value</Value>
									</CustomProperty>
									<CustomProperty>
										<Name>calendar:Event.EndDate</Name>
										<Value>=Fields!EndTime.Value</Value>
									</CustomProperty>
									<CustomProperty>
										<Name>calendar:Event.StartDate</Name>
										<Value>=Fields!StartTime.Value</Value>
									</CustomProperty>
									<CustomProperty>
										<Name>calendar:Event.Backcolor</Name>
										<Value>=IIF(ToString(Fields!Value.Value).contains("..."), "Coral", "LightGreen")</Value>
									</CustomProperty>
									<CustomProperty>
										<Name>calendar:Event.FontSize</Name>
										<Value>10pt</Value>
									</CustomProperty>
									<CustomProperty>
										<Name>calendar:Event.FontWeight</Name>
										<Value>Bold</Value>
									</CustomProperty>
									<CustomProperty>
										<Name>calendar:Event.BorderColor</Name>
										<Value>Red</Value>
									</CustomProperty>
									<CustomProperty>
										<Name>calendar:Action.BookmarkLink</Name>
										<Value>=Fields!Value.Value</Value>
									</CustomProperty>
								</CustomProperties>
							</DataGrouping>
						</DataGroupings>
					</DataRowGroupings>
				</CustomData>
			</CustomReportItem>
			<List Name="List1">
				<DataSetName>DataSet1</DataSetName>
				<Height>1.0675cm</Height>
				<Left>3.175cm</Left>
				<ReportItems>
					<Textbox Name="TextBox1">
						<Bookmark>=Fields!Value.Value</Bookmark>
						<CanGrow>true</CanGrow>
						<Height>0.635cm</Height>
						<Style>
							<PaddingBottom>2pt</PaddingBottom>
							<PaddingLeft>2pt</PaddingLeft>
							<PaddingRight>2pt</PaddingRight>
							<PaddingTop>2pt</PaddingTop>
						</Style>
						<Value>=Fields!Value.Value</Value>
						<Visibility>
							<Hidden>False</Hidden>
						</Visibility>
						<Width>4.7625cm</Width>
					</Textbox>
				</ReportItems>
				<Top>0.3175cm</Top>
				<Visibility>
					<Hidden>False</Hidden>
				</Visibility>
				<Width>8.89cm</Width>
				<ZIndex>1</ZIndex>
			</List>
		</ReportItems>
	</Body>
	<Width>16cm</Width>
	<DataSources>
		<DataSource Name="DataSource1">
			<ConnectionProperties>
				<ConnectString>123</ConnectString>
				<DataProvider>TESTING</DataProvider>
			</ConnectionProperties>
		</DataSource>
	</DataSources>
	<DataSets>
		<DataSet Name="DataSet1">
			<Fields>
				<Field Name="StartTime">
					<DataField>StartTime</DataField>
				</Field>
				<Field Name="EndTime">
					<DataField>EndTime</DataField>
				</Field>
				<Field Name="Value">
					<DataField>Value</DataField>
				</Field>
			</Fields>
			<Query>
				<CommandText>StartTime(datetime),EndTime(datetime),Value(string)
2007-12-05,2007-12-07,"www.google.com"
2007-12-06,2007-12-07,"www.rambler.ru"
2007-12-07,2007-12-07,"www.ya.ru"
2008-02-05,2008-03-07,"www.yahoo.com"</CommandText>
				<DataSourceName>DataSource1</DataSourceName>
			</Query>
		</DataSet>
	</DataSets>
</Report>
